{
    "name": "inspectorphp/inspector",
    "description": "Inspector",
    "keywords": [
        "inspector",
        "console",
        "debugger",
        "log",
        "console log"
    ],
    "type": "library",
    "require": {
        "php": "^8.3",
        "symfony/console": "^7.3",
        "nikic/php-parser": "^5.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4",
        "laravel/pint": "^1.25",
        "rector/rector": "^2.2",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inspector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "bin": [
        "bin/inspector"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
